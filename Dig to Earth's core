-- ========== WORLDS AUTOFARM TAB ==========
local WorldsTab = Window:CreateTab("üåç World AutoFarm üåç", 4483362458)

local worldToggles = {}

for i = 1, 10 do
    worldToggles[i] = false
    WorldsTab:CreateToggle({
        Name = "üåé AutoFarm World " .. i,
        CurrentValue = false,
        Callback = function(state)
            worldToggles[i] = state
            if state then
                task.spawn(function()
                    local player = game.Players.LocalPlayer
                    local char = player.Character or player.CharacterAdded:Wait()
                    local hrp = char:WaitForChild("HumanoidRootPart")
                    local humanoid = char:WaitForChild("Humanoid")
                    local worldName = "World" .. i
                    local target = workspace:WaitForChild(worldName):WaitForChild("WinPart")
                    while worldToggles[i] do
                        task.wait(0.5)
                        -- Hide character
                        for _, part in pairs(char:GetChildren()) do
                            if part:IsA("BasePart") then
                                part.Transparency = 1
                                part.CanCollide = false
                            end
                        end
                        humanoid.PlatformStand = true

                        -- Teleport to the platform
                        hrp.CFrame = target.CFrame + Vector3.new(0, 5, 0)

                        -- Trigger touch event
                        for _, touch in pairs(target:GetChildren()) do
                            if touch:IsA("TouchTransmitter") or touch.ClassName == "TouchInterest" then
                                local partTouched = touch.Parent
                                if partTouched and partTouched:IsA("BasePart") then
                                    firetouchinterest(hrp, partTouched, 0)
                                    task.wait(0.05)
                                    firetouchinterest(hrp, partTouched, 1)
                                end
                            end
                        end

                        -- Show character
                        for _, part in pairs(char:GetChildren()) do
                            if part:IsA("BasePart") then
                                part.Transparency = 0
                                part.CanCollide = true
                            end
                        end
                        humanoid.PlatformStand = false

                        print("üåç AutoFarm World " .. i .. " is running!")
                    end
                end)
            else
                print("‚è∏Ô∏è AutoFarm World " .. i .. " stopped!")
            end
        end
    })
end
