    Name = "üíé Gem Blaster (Max Gems Farming)",
    CurrentValue = false,
    Callback = function(state)
        gemBlaster = state
        while gemBlaster do
            local args = {5} -- ID for gems
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SpinPrizeEvent"):FireServer(unpack(args))
            task.wait(0.1)
        end
    end,
})

local cashMultiplier = false
AutoFarmTab:CreateToggle({
    Name = "üí∞ 10x Cash Turbo",
    CurrentValue = false,
    Callback = function(state)
        cashMultiplier = state
        while cashMultiplier do
            local args = {8} -- ID for 10x cash
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SpinPrizeEvent"):FireServer(unpack(args))
            task.wait(0.1)
        end
    end,
})

local freeUpgrade = false
AutoFarmTab:CreateToggle({
    Name = "üß† Free Auto Upgrade (ALL STATS)",
    CurrentValue = false,
    Callback = function(state)
        freeUpgrade = state
        while freeUpgrade do
            local upgrades = {
                {"PetLuck", 0},
                {"GemChance", 0},
                {"Strength", 0},
                {"WalkSpeed", 0}
            }
            for _, upgrade in ipairs(upgrades) do
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("UpgradeEvent"):FireServer(unpack(upgrade))
            end
            task.wait(0.1)
        end
    end,
})

